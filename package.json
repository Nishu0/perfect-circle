    "clean": "rm -f *.png *.jpg *.jpeg *.gif *.webp *.svg *.ico"
  },
  "keywords": [
    "game",
    "circle",
    "drawing",
    "canvas",
    "mobile",
    "touch"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/perfect-circle-game.git"
  },
  "homepage": "https://your-perfect-circle-game.vercel.app",
  "dependencies": {
    "@farcaster/miniapp-sdk": "^0.1.8"
  }
}
