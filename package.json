{
  "name": "perfect-circle-game",
  "version": "1.0.0",
  "description": "A web game where players draw circles and get scored on how close they are to a perfect circle",
  "main": "index.html",
  "scripts": {
    "dev": "cp public/*.png . && python3 -m http.server 8000",
    "start": "cp public/*.png . && python3 -m http.server 8000",
